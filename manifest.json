{
  "name": "Feedly はてブ",
  "version": "0.9.1",
  "manifest_version": 2,
  "description": "はてなブックマークの情報を Feedly 上に表示します。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://feedly.com/*"],
      "css": ["css/style.css"],
      "js": ["js/script.js"]
    }
  ],
  "options_page": "html/options.html",
  "permissions": [
    "storage"
  ]
}
